{
  "aws": {
    "name": "aws",
    "region": "us-east-1",
    "runtime": "python3.8",
    "plugins": [
      "serverless-dynamodb",
      "serverless-offline"
    ]
  },
  "azure": {
    "region": "eastus",
    "runtime": "python3.8",
    "plugins": [ "serverless-azure-functions" ]
  },
  "gcp": {
    "region": "us-central1",
    "runtime": "python3.8",
    "plugins": [
      "serverless-google-cloudfunctions"
    ]
  }
}
